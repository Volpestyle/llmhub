{
  "name": "@volpestyle/llmhub-node",
  "version": "0.1.0",
  "description": "Cross-provider LLM registry and inference adapter for Node.js",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
