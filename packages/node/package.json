{
  "name": "@volpestyle/ai-kit-node",
  "version": "0.1.0",
  "description": "Cross-provider model registry and inference adapter for Node.js",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./testing": {
      "import": "./dist/testing/index.js",
      "require": "./dist/testing/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "pnpm build",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "refresh:models": "tsx scripts/refreshScrapedModels.ts"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.14.0",
    "dotenv": "^16.4.5",
    "playwright": "^1.45.0",
    "tsx": "^4.16.2",
    "tsup": "^8.0.1",
    "typescript": "^5.4.0",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "packageManager": "pnpm@9.0.0+sha256.bdfc9a7b372b5c462176993e586492603e20da5864d2f8881edc2462482c76fa"
}
